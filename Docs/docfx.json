{
    "metadata": [
        {
            "src": [
                {
                    "src": "../Realm/Realm",
                    "files": [
                        "Realm.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "_site/**"
                    ]
                }
            ],
            "dest": "obj/temp/api",
            "filter": "filterConfig.yml"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.yml"
                ],
                "src": "obj/temp/api",
                "dest": "reference"
            },
            {
                "files": [
                    "linqsupport.md",
                    "toc.yml",
                    "index.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            },
            {
                "files": "CHANGELOG.md",
                "src": ".."
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apispec/*.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_enableSearch": true,
            "_appTitle": "Realm",
            "_appFaviconPath": "images/favicon.ico",
            "_appLogoPath": "images/logo.svg",
            "_appFooter": "Copyright Â© 2020-2023 <a href=\"https://realm.io\">Realm</a><br>Generated by <strong>DocFX</strong>",
            "_gitContribute": {
                "repo": "https://github.com/realm/realm-dotnet.git",
                "branch": "main",
                "path": "Docs/apispec"
            }
        },
        "fileMetadataFiles": [],
        "fileMetadata": {
            "_disableBreadcrumb": {
                "linqsupport.md": true,
                "index.md": true,
                "../CHANGELOG.md": true
            }
        },
        "template": [ "default", "template" ],
        "postProcessors": [],
        "noLangKeyword": false,
        "properties": {
            "TargetFramework": "net6.0"
        }
    }
}