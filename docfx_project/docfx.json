{
	"metadata": [
		{
			"src": [
				{
					"src": "../Platform.XamarinIOS/",
					"files": [
						"Realm.XamarinIOS/Realm.XamarinIOS.csproj",
						"Realm.Sync.XamarinIOS/Realm.Sync.XamarinIOS.csproj"
					],
					"exclude": [
						"**/obj/**",
						"**/bin/**",
						"_site/**"
					]
				}
			],
			"dest": "api",
			"filter": "docfx_project/filterConfig.yml"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"articles/**.md",
					"articles/**/toc.yml",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"dest": "_site",
		"globalMetadataFiles": [],
		"globalMetadata": {
			"_enableSearch": true,
			"_appTitle": "Realm"
		},
		"fileMetadataFiles": [],
		"template": [
			"templates/realm",
			"default"
		],
		"postProcessors": [],
		"noLangKeyword": false
	}
}