{
	"metadata": [
		{
			"src": [
				{
					"src": "../Platform.PCL/",
					"files": [
						"Realm.PCL/Realm.PCL.csproj",
						"Realm.Sync.PCL/Realm.Sync.PCL.csproj"
					],
					"exclude": [
						"**/obj/**",
						"**/bin/**",
						"_site/**"
					]
				}
			],
			"dest": "obj/temp/api",
			"filter": "docfx_project/filterConfig.yml"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"**/*.yml"
				],
				"src": "obj/temp/api",
				"dest": "api"
			},
			{
				"files": [
					"linqsupport/*.md",
					"toc.yml",
					"*.md",
					"api/index.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			},
			{
				"files": "CHANGELOG.md",
				"src": "..",
				"dest": "changelog"
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apispec/*.md"
				],
				"exclude": [
					"obj/**",
					"_site/**"
				]
			}
		],
		"xref": [
      		"../packages/msdn.4.5.2.0.1.0-alpha-1611021200/content/msdn.4.5.2.zip"
    	],
		"dest": "_site",
		"globalMetadataFiles": [],
		"globalMetadata": {
			"_enableSearch": true,
			"_appTitle": "Realm",
			"_appFaviconPath": "images/favicon.ico"
		},
		"fileMetadataFiles": [],
		"template": [ "default", "templates/realm" ],
		"postProcessors": [],
		"noLangKeyword": false
	}
}